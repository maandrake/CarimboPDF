{
  "CarimboPDF": {
    "VBAIntegration": {
      "ApiSettings": {
        "BaseUrl": "http://127.0.0.1:8080",
        "Timeout": 30000,
        "MaxRetries": 3,
        "EnableLogging": true
      },
      "DetranSettings": {
        "DefaultUF": "SC",
        "SupportedStates": [
          "AC", "AL", "AP", "AM", "BA", "CE", "DF", "ES", "GO", "MA",
          "MT", "MS", "MG", "PA", "PB", "PR", "PE", "PI", "RJ", "RN",
          "RS", "RO", "RR", "SC", "SP", "SE", "TO"
        ],
        "SessionTimeout": 3600,
        "HeadlessMode": true
      },
      "WordIntegration": {
        "AutoInsertData": false,
        "FormatStyle": "default",
        "InsertTimestamp": true,
        "UseTableFormat": false
      },
      "ErrorHandling": {
        "ShowDetailedErrors": false,
        "LogErrors": true,
        "RetryOnFailure": true,
        "MaxRetryAttempts": 2
      }
    }
  },
  "Logging": {
    "Level": "Information",
    "Providers": {
      "File": {
        "Enabled": true,
        "Path": "logs/carimbopdf-vba.log",
        "MaxSizeBytes": 10485760,
        "MaxFiles": 5
      },
      "Console": {
        "Enabled": true
      }
    }
  }
}